apiVersion: v1
kind: Secret
metadata:
  name: fproject-backend-secret
  namespace: default
type: Opaque
data:
  # Base64 encoded values - DO NOT COMMIT REAL VALUES
  # To create base64 encoded values:
  # echo -n 'your_password' | base64
  DB_PASSWORD: <BASE64_ENCODED_PASSWORD>
  GOOGLE_CLIENT_SECRET: <BASE64_ENCODED_CLIENT_SECRET>
